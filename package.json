{
  "name": "express-api-starter-ts",
  "version": "1.2.1",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "pm2:prod": "pm2 start processes.json --watch -i max --no-daemon",
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "build:image": "rm -rf ./dist/ && tsc && docker build -t mtsammy40/crypto_alerts_api:latest -t mtsammy40/crypto_alerts_api:$npm_package_version .",
    "start:dist": "node dist/src/index.js",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Samuel Mutemi",
  "repository": {
    "type": "git",
    "url": "https://github.com/mtsammy40/crypto_alerts_api"
  },
  "license": "MIT",
  "dependencies": {
    "@novu/node": "^0.24.2",
    "ajv": "^8.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "mongodb": "^6.6.2",
    "morgan": "^1.10.0",
    "pm2": "^5.4.0",
    "redis": "^4.6.14",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.6",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  }
}
